package java8concept;

public class Pay {

	public void creditPay() {
		System.out.println("Instance Method");
	}
	
	
	
	
	public int debit() {
		System.out.println("Instance Method");
		return 0;
	}
	
	
	public int credit() {
		System.out.println("Instance Method");
		System.out.println("Instance Method");
		System.out.println("Instance Method");

		return 0;
	}

}
