package innerexp;

class OuterClass {
	  int x = 10;

	  private  class InnerClass {
	    int y = 5;
	    OuterClass myOuter = new OuterClass();
	    myOuter.
	  }
	}

	public class Main {
	  public static void main(String[] args) {
	    OuterClass.InnerClass myInner = myOuter.new InnerClass();
	    System.out.println(myInner.y + myOuter.x);
	  }
	}